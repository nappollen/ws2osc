{
  "name": "ws2osc",
  "version": "1.0.0",
  "description": "WebSocket to OSC bridge with OSCQuery support",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx main.ts",
    "build:all": "npm run build && npm run caxa:all",
    "caxa:all": "npm run caxa:win && npm run caxa:linux && npm run caxa:mac",
    "caxa:win": "npx caxa --input . --output build/ws2osc-win.exe -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/dist/main.js\"",
    "caxa:linux": "npx caxa --input . --output build/ws2osc-linux -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/dist/main.js\"",
    "caxa:mac": "npx caxa --input . --output build/ws2osc-macos -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/dist/main.js\""
  },
  "keywords": [
    "websocket",
    "osc",
    "oscquery"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "node-osc": "^9.1.7",
    "oscquery": "^1.1.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-osc": "^9.1.0",
    "@types/ws": "^8.5.10",
    "caxa": "^3.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3"
  },
  "pkg": {
    "assets": [
      "node_modules/ws/**/*",
      "node_modules/oscquery/**/*",
      "node_modules/node-osc/**/*"
    ],
    "scripts": [],
    "outputPath": "build"
  }
}
